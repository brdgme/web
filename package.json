{
  "name": "brdgme-web",
  "version": "0.1.0",
  "description": "Web interface for brdg.me",
  "repository": "https://github.com/brdgme/web.git",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "build-prod": "node_modules/.bin/webpack -p",
    "clean": "rm -rf dist test-build",
    "test": "node_modules/.bin/tsc --outDir test-build test/test.ts && node_modules/.bin/mocha test-build/test/test.js",
    "watch": "node_modules/.bin/webpack -w",
    "server": "node server.js",
    "dev": "npm run watch & npm run server & npm run websocket",
    "websocket": "cd node_modules/brdgme-websocket-server && npm start"
  },
  "author": "Michael Alexander <beefsack@gmail.com>",
  "license": "GPL-3.0",
