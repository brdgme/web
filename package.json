    "@types/react-redux": "^4.4.40",
    "@types/redux-actions": "^1.2.4",
    "@types/redux-immutable": "^3.0.33",
    "@types/superagent": "^2.0.36",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.1",
    "superagent": "^3.5.2",
    "url": "^0.11.0"
