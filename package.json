    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "source-map-loader": "^0.2.0",
